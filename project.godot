config_version=5

[application]

config/name="Silver Window"
run/main_scene="res://main_menu.tscn"
config/features=PackedStringArray("4.0", "GL Compatibility")
run/max_fps=60
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/fullsize=false
config/icon="res://assets/MainMenu/eyes-1.png"

[autoload]

DialogueManager="*res://singletons/dialogue_manager.gd"
SceneSwitcher="*res://singletons/scene_switcher.gd"
GlobalData="*res://singletons/global_data.gd"
TransitionManager="*res://singletons/transition_manager.tscn"
SoundManager="*res://singletons/sound_manager.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=4
window/size/resizable=false
window/stretch/mode="canvas_items"

[editor]

movie_writer/movie_file="/home/mark/Videos/GodotMovieWriter/SilverWindow/silver-window-demo.avi"
version_control/autoload_on_startup=true

